<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="rates-create" author="nortin" context="dml">
        <createTable tableName="rate">
            <column name="curId" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="date" type="DATE"/>
            <column name="curAbbreviation" type="VARCHAR(255)"/>
            <column name="curScale" type="INTEGER"/>
            <column name="curName" type="VARCHAR(255)"/>
            <column name="curOfficialRate" type="DOUBLE"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
