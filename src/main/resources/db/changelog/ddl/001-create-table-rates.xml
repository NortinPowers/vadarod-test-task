<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="rates-create" author="nortin" context="dml">
        <createTable tableName="rates">
            <column name="cur_id" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="date" type="DATE"/>
            <column name="cur_abbreviation" type="VARCHAR(255)"/>
            <column name="cur_scale" type="INTEGER"/>
            <column name="cur_name" type="VARCHAR(255)"/>
            <column name="cur_official_Rate" type="DOUBLE"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
